double value;
value = Stick.getX();
value = Stick.getY();
value = Stick.getZ();
value = Stick.getThrottle();
value = Stick.getTwist();

boolean buttonValue;
buttonValue = Stick.getTop();
buttonValue = Stick.getTrigger();
